{
    "name": "my-markdown-viewer",
    "displayName": "My Markdown Viewer",
    "description": "Simple Markdown preview for editing .md files.",
    "version": "0.0.1",
    "engines": {
      "vscode": "^1.50.0"
    },
    "activationEvents": [
      "onLanguage:markdown"
    ],
    "main": "extension.js",
    "contributes": {
      "commands": [
        {
          "command": "markdownViewer.openPreview",
          "title": "Open Markdown Preview"
        }
      ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/g2developer/markdown_editor"
    }
  }
  